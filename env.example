# Настройки базы данных PostgreSQL
DB_NAME=your_db_name_here
DB_USER=your_db_user_here
DB_PASSWORD=your_db_password_here
DB_HOST=your_db_host_here
DB_PORT=your_db_port_here

# Основные настройки Flask
SECRET_KEY=your_secret_key_here
FLASK_ENV=development
FLASK_DEBUG=1

# WTF forms
WTF_CSRF_SECRET_KEY=your_csrf_secret_key_here
WTF_CSRF_ENABLED=True

# JWT настройки
JWT_SECRET_KEY=your_secret_key_here

# Шифрование
ENCRYPTION_KEY=your_encryption_key_here
ENCRYPTION_OPTIONS=your_encryption_options_here

# Redis настройки (опционально)
REDIS_URL=redis://localhost:6379/0

# Настройки принтеров ESC/POS
KITCHEN_PRINTER_HOST=192.168.1.100
KITCHEN_PRINTER_PORT=9100
BAR_PRINTER_VENDOR_ID=0x0483
BAR_PRINTER_PRODUCT_ID=0x5740
RECEIPT_PRINTER_HOST=192.168.1.101
RECEIPT_PRINTER_PORT=9100
